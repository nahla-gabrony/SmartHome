@{
    ViewData["Title"] = "Not Found";
}

<section>
    <div class="container position-relative px-3 main-container">
        <div class="page-notfound">
            <div class="notfound">
                <div class="notfound-404">
                    <h1>Oops!</h1>
                </div>
                <h2>404 - Page Not Found</h2>
                <p>The page you are looking for might have been removed or its name changed or is temporarily unavailable.</p>
                <a class="btn btn-main" asp-controller="Home" asp-action="index">Go To Homepage</a>
            </div>
        </div>
    </div>
</section>